#include<stdio.h>

void main()

{	int score,s;

    char grade;

	printf("please enter score:");

	scanf("%d",&score);

	s=score/10;

		switch(s)

	{

	case 0:

	case 1:

	case 2:

	case 3:

	case 4:

	case 5:grade='E';break;

	case 6:grade='D';break;

	case 7:grade='C';break;

	case 8:grade='B';break;

	case 9:grade='A';

    case 10:grade='A';



	}

	

	printf("grade is %c\n",grade);

}
